#include <stdio.h>
#include<string.h>
int main()
{
    char str[105];
    int h=0,l=0,flag=0;
    gets(str);
    l=strlen(str)-1;
    while(l>h)
    {
        if(str[h++]!=str[l--])
        {  
            printf("NO");
            flag=1;
            break;
        }
    }     
    if(flag==0)
     printf("YES");
}     
